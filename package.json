{
  "name": "ng-image-viewer",
  "version": "0.1.7",
  "description": "Image viewer component for angular2",
  "keywords": [
    "angular",
    "angular2",
    "image",
    "image-viewer",
    "rotate",
    "coordinate"
  ],
  "homepage": "https://github.com/Redface/ng-image-viewer",
  "main": "index.js",
  "scripts": {
    "start": "npm run watch",
    "start:demo": "npm run server:demo && npm run watch:demo",
    "clean": "./node_modules/.bin/rimraf node_modules doc typings coverage dist && npm cache clean",
    "clean:dist": "./node_modules/.bin/rimraf ./dist",
    "clean:install": "npm install && tsd install",
    "clean:start": "npm start",
    "watch": "./node_modules/.bin/webpack --config client-config/webpack.js --watch --progress --profile --colors --display-error-details --display-cached",
    "watch:demo": "./node_modules/.bin/webpack --config client-config/webpack-demo.js --watch --progress --profile --colors --display-error-details --display-cached",
    "build": "./node_modules/.bin/webpack --config client-config/webpack.js --progress --profile --colors --display-error-details --display-cached",
    "build:demo": "./node_modules/.bin/webpack --config client-config/webpack-demo.js --progress --profile --colors --display-error-details --display-cached",
    "server:demo": "webpack-dev-server --config client-config/webpack-demo.js --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base demo/dist"
  },
  "author": {
    "name": "Daehwan Lee",
    "email": "redface0512@gmail.com",
    "url": "https://github.com/jazzyredface"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Redface/ng-image-viewer.git"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "^2.1.2",
    "@angular/compiler": "^2.1.2",
    "@angular/core": "^2.1.2",
    "@angular/platform-browser": "^2.1.2",
    "@angular/platform-browser-dynamic": "^2.1.2",
    "@types/core-js": "^0.9.34",
    "@types/node": "^6.0.46",
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "^2.2.4",
    "browser-sync": "^2.17.5",
    "browser-sync-webpack-plugin": "^1.1.3",
    "codelyzer": "0.0.28",
    "copy-webpack-plugin": "^3.0.1",
    "core-js": "^2.4.1",
    "html-webpack-plugin": "^2.24.0",
    "node-sass": "^3.10.1",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.0-rc.1",
    "sass-loader": "^4.0.2",
    "source-map-loader": "^0.1.5",
    "ts-helpers": "^1.1.2",
    "ts-node": "^1.6.1",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.0.6",
    "typings": "^0.8.1",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^0.14.1",
    "zone.js": "^0.6.26"
  }
}
